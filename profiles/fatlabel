abi <abi/3.0>,

include <tunables/global>

@{exec_path} = /{usr/,}sbin/fatlabel
profile fatlabel @{exec_path} {
  include <abstractions/base>
  include <abstractions/disks-write>

  @{exec_path} mr,

  include if exists <local/fatlabel>
}
