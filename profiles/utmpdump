abi <abi/3.0>,

include <tunables/global>

@{exec_path} = /{usr/,}bin/utmpdump
profile utmpdump @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  /var/log/wtmp{,.[0-9]*} r,
  /var/log/btmp{,.[0-9]*} r,

  include if exists <local/utmpdump>
}
