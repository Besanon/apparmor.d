abi <abi/3.0>,

include <tunables/global>

@{exec_path}  = /{usr/,}lib/gvfs/gvfsd-afc
@{exec_path} += /usr/libexec/gvfsd-afc
profile gvfsd-afc @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/gvfsd-afc>
}
