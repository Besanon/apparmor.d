abi <abi/3.0>,

include <tunables/global>

@{exec_path}  = /{usr/,}lib/gvfs/gvfsd-dav
@{exec_path} += /usr/{lib,libexec}/gvfsd-dav
profile gvfsd-dav @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/gvfsd-dav>
}
