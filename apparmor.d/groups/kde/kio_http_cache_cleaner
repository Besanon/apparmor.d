abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/kf5/kio_http_cache_cleaner
profile kio_http_cache_cleaner @{exec_path} {
  include <abstractions/base>

  @{exec_path} mr,

  include if exists <local/kio_http_cache_cleaner>
}