abi <abi/3.0>,

include <tunables/global>

@{exec_path} = @{lib}/kauth/fontinst
profile kauth-fontinst @{exec_path} {
  include <abstractions/base>
  include <abstractions/qt5>

  @{exec_path} mr,

  /usr/share/icu/[0-9]*.[0-9]*/*.dat r,

  include if exists <local/kauth-fontinst>
}